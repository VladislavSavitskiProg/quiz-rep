{% extends 'base.html.twig' %}

{% block title %}Hello Quizes{% endblock %}

{% block body %}
<div class="example-wrapper">
    <h1>Chose question</h1>

    {% for question in questions %}
        <h4>{{ question }}</h4>
        <p>
            <a href="{{ path('question', { id: question.id }) }}">View</a>
        </p>
    {% endfor %}

</div>
{% endblock %}
